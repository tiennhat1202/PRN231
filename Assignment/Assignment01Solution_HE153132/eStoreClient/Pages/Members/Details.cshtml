@page
@model eStoreClient.Pages.Members.DetailsModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "MemberDetails";
}

<h1>Details Member</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(Model => Model.getMember!.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(Model => Model.getMember!.Email)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(Model => Model.getMember!.CompanyName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(Model => Model.getMember!.CompanyName)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(Model => Model.getMember!.City)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(Model => Model.getMember!.City)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(Model => Model.getMember!.Country)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(Model => Model.getMember!.Country)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(Model => Model.getMember!.Password)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(Model => Model.getMember!.Password)
        </dd>
